<?xml version="1.0" encoding="UTF-8"?>
<configuration>
        <property>
                <name>inputFilePath</name>
                <value>/yz_newland_base/bigdata/gn_xdr/joinData/data/</value>
                <description>汇总源数据的输入路径（测试目录：/bigData/testStatistic/）（上线目录：/bigData/detailToHbaseForStatis/）</description>
        </property>
        <property>
                <name>filterFileName</name>
                <value>-r-</value>
                <description>输入路径下过滤包含某些字符串的文件名</description>
        </property>
        <property>
                <name>outputFilePath</name>
                <value>/yz_newland_base/bigdata/gn_xdr/statistic/statisticMiddleTable/ft_mid_gn_Collect_daily</value>
                <description>mapreduce工作完成后文件的输出目录的前缀，目录还要加UTAP的时间点</description>
        </property>
        <property>
                <name>outputFilePathForGp</name>
                <value>/yz_newland_base/bigdata/gn_xdr/statistic/middleTable/ft_mid_gn_Collect_daily/</value>
                <description>mapreduce程序完成后文件中间文件移动的最终目的目录，是入GP的源数据目录</description>
        </property>
        <property>
                <name>sourceDataLength</name>
                <value>33</value>
                <description>汇总源数据的总字段数</description>
        </property>
        <property>
                <name>ifDeleteSourceData</name>
                <value>false</value>
                <description>汇总的源数据是否删除</description>
        </property>
        <property>
                <name>mapred.job.queue.name</name>
                <value>hadoop</value>
                <description>指定job对应的队列名（上线队列：hadoop）(测试队列：other)</description>
        </property>
        <property>
                <name>notIncludeFileName</name>
                <value>_SUCCESS</value>
                <description>汇总源文件排除的文件名</description>
        </property>
        <property>
                <name>jobNamePrefix</name>
                <value>StatisticIpHttpForXdrJob</value>
                <description>汇总源文件排除的文件名</description>
        </property>
        <property>
                <name>maxStringLength</name>
                <value>4000</value>
                <description>String变量存的最大长度（数字）</description>
        </property>
        <property>
                <name>exceptionRecord</name>
                <value>exceptionRecord</value>
                <description>异常记录文件名前缀</description>
        </property>
        <property>
                <name>dirOfData</name>
                <value>data2,data3,data4,data6,data7,data8,data9,data10,data11,data12,data13</value>
                <description>数据目录data1,data2,data3,data4,data5,data6,data7,data8,data9,data10,data11,data12,data13,data14</description>
        </property>
</configuration>